<odoo>

    <record id="vat_5_exclude" model="account.tax">
        <field name="name">CONS - Tax 5% (Price Excl.)</field>
        <field name="amount_type">percent</field>
        <field name="amount">5.0</field>
        <field name="price_include" eval="False" />
        <field name="company_id" ref="base.main_company" />
    </record>

    <record id="vat_5_include" model="account.tax">
        <field name="name">CONS - Tax 5% (Price Incl.)</field>
        <field name="amount_type">percent</field>
        <field name="amount">5.0</field>
        <field name="price_include" eval="True" />
        <field name="company_id" ref="base.main_company" />
    </record>

    <record id="vat_20_exclude" model="account.tax">
        <field name="name">CONS - Tax 20% (Price Excl.)</field>
        <field name="amount_type">percent</field>
        <field name="amount">20.0</field>
        <field name="price_include" eval="False" />
        <field name="company_id" ref="base.main_company" />
    </record>

    <record id="tax_5_consignor_1" model="account.tax">
        <field name="name">CONS - Tax 5% (Price Excl.) (Consignor 1)</field>
        <field name="amount">5.0</field>
        <field name="amount_type">percent</field>
        <field name="price_include" eval="False" />
        <field name="account_id" ref="account_consignor_1"/>
        <field name="refund_account_id" ref="account_consignor_1"/>
    </record>

    <record id="tax_20_consignor_1" model="account.tax">
        <field name="name">CONS - Tax 20% (Price Excl.) (Consignor 1)</field>
        <field name="amount">20.0</field>
        <field name="amount_type">percent</field>
        <field name="price_include" eval="False" />
        <field name="account_id" ref="account_consignor_1"/>
        <field name="refund_account_id" ref="account_consignor_1"/>
    </record>

</odoo>
