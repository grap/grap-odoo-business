<odoo>

    <record id="view_res_cuntry_group_form" model="ir.ui.view" >
        <field name="model">res.country.group</field>
        <field name="inherit_id" ref="base.view_country_group_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='country_group']" position="before">
                <group name="organic_informations">
                    <field name="european_classification"/>
                </group>
            </xpath>
        </field>
    </record>

</odoo>
