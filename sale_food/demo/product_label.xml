<odoo>

    <record id="label_agriculture_biologique" model="product.label">
        <field name="name">Agriculture Biologique</field>
        <field name="code">AB</field>
        <field name="website">http://agriculture.gouv.fr/lagriculture-biologique-ab</field>
        <field name="minimum_organic_notation">4</field>
        <field name="mandatory_on_invoice" eval="True" />
        <field name="image" type="base64" file="sale_food/static/img/label_agriculture_biologique-image.png"/>
    </record>

    <record id="label_max_havelaar" model="product.label">
        <field name="name">Max Havelaar</field>
        <field name="code">MH</field>
        <field name="minimum_social_notation">3</field>
        <field name="image" type="base64" file="sale_food/static/img/label_max_havelaar-image.png"/>
    </record>

    <record id="label_gluten_free" model="product.label">
        <field name="name">Gluten Free</field>
        <field name="code">GF</field>
        <field name="image" type="base64" file="sale_food/static/img/label_gluten_free-image.png"/>
    </record>

    <record id="label_alcool_grossesse" model="product.label">
        <field name="name">Alcool pendant la grossesse</field>
        <field name="code">ALC</field>
        <field name="website">http://inpes.santepubliquefrance.fr/70000/cp/15/cp152908-alcool-grossesse-risque.asp</field>
        <field name="is_alcohol" eval="True" />
        <field name="image" type="base64" file="sale_food/static/img/label_alcool_grossesse-image.png"/>
    </record>

</odoo>
